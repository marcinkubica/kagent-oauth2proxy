# OAuth2 Proxy Secrets Configuration
# Copy this file to 'secrets.yaml' and fill in your actual values
# The secrets.yaml file is automatically ignored by git

# You can ignore this file if you choose to use setup wizard

# GitHub OAuth App Credentials (replace with your actual credentials):
oauth2:
  clientId: "your_github_oauth_client_id_20_chars"
  clientSecret: "your_github_oauth_client_secret"
  cookieSecret: "your_32_byte_cookie_secret_use_python_secrets_token_urlsafe_32"

# Example values
# oauth2:
#   clientId: "Iv1...."
#   clientSecret: "16C7...."
#   cookieSecret: "YqBgG_...."

# To generate a secure cookie secret, run:
# python3 -c 'import secrets; print(secrets.token_urlsafe(32))'

# OAuth App Configuration URLs (for reference):
# Local Development:
#   Homepage URL: http://localhost:8090
#   Callback URL: http://localhost:8090/oauth2/callback
#
# Production:
#   Homepage URL: https://yourdomain.com
#   Callback URL: https://yourdomain.com/oauth2/callback 
